{
  "name": "react-npm-package-template",
  "version": "0.0.0",
  "description": "Npm package creation base using React",
  "files": [
    "dist/*"
  ],
  "types": "dist/index.d.ts",
  "style": "dist/index.css",
  "main": "src/index.js",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs.js"
    },
    "./styles.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsup --watch",
    "build": "tsup && npm run tailwind --minify -o ./dist/index.css",
    "tailwind": "npx tailwindcss -i ./src/lib/styles.css -o ./dist/index.css",
    "start": "react-scripts start"
  },
  "author": "johnnyferreiradev",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^20.6.3",
    "@types/react": "^18.2.22",
    "i18next": "^23.5.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.2",
    "react-scripts": "^5.0.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.30",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.1",
    "tailwindcss": "^3.3.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
